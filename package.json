{
  "name": "ag-dev",
  "version": "2.0.0",
  "description": "Multi-Agent Development Orchestration Platform",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "dev": "node --watch server/server.js",
    "build:ui": "cd ui && npx vite build --outDir ../ui-dist",
    "build": "npm run build:ui",
    "superskills": "node superskills/runner.js",
    "superskills:list": "node superskills/runner.js list",
    "superskills:stats": "node superskills/runner.js stats",
    "docker:build": "docker build -t ag-dev .",
    "docker:run": "docker-compose up -d",
    "install-all": "npm install && cd ui && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "ws": "^8.14.2",
    "node-pty": "^1.0.0",
    "js-yaml": "^4.1.0",
    "uuid": "^9.0.1"
  },
  "keywords": ["multi-agent", "development", "orchestration", "ai"],
  "author": "AG Dev Team",
  "license": "MIT"
}