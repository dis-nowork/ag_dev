{
  "_comment": "AG Dev V3 â€” OpenClaw Edition. Configuration for tmux-based multi-agent orchestration.",
  "tmux": {
    "socket": "/tmp/agdev.sock",
    "maxSessions": 16
  },
  "agents": {
    "configPath": "./agents",
    "available": [
      "analyst", "architect", "dev", "qa", "devops", "pm", "po",
      "ux", "data-engineer", "scrum-master", "content-writer", "seo-analyst"
    ]
  },
  "workflows": {
    "path": "./workflows",
    "available": [
      "greenfield-fullstack", "greenfield-service", "greenfield-ui",
      "brownfield-fullstack", "brownfield-discovery",
      "qa-loop", "spec-pipeline", "code-review"
    ]
  },
  "squads": {
    "path": "./squads"
  },
  "memory": {
    "baseDir": "./memory",
    "tiers": ["hot", "warm", "cold"]
  },
  "qualityGates": {
    "enforced": true,
    "autoQA": true,
    "checks": ["lint", "test", "security"]
  },
  "qaLoop": {
    "maxIterations": 5
  },
  "handoff": {
    "dir": ".agdev/handoff"
  }
}
