{
  "name": "schema-to-types",
  "version": "1.0.0",
  "category": "generator",
  "description": "Converts JSON Schema objects to TypeScript interfaces with support for nested objects, arrays, enums, and optional fields.",
  "input": {
    "type": "json",
    "schema": {
      "type": "object",
      "properties": {
        "schema": {
          "type": "object",
          "description": "JSON Schema object to convert"
        },
        "interfaceName": {
          "type": "string",
          "description": "Name for the root interface",
          "default": "Generated"
        }
      },
      "required": ["schema"]
    },
    "example": {
      "schema": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "age": { "type": "number" },
          "email": { "type": "string", "format": "email" }
        },
        "required": ["name"]
      },
      "interfaceName": "User"
    }
  },
  "output": {
    "type": "json",
    "format": "{ typescript: string, interfaces: string[] }"
  },
  "tokenSavings": "Saves 300-800 tokens by generating TypeScript interfaces instead of manual conversion",
  "tags": ["typescript", "json-schema", "types", "interfaces", "codegen"],
  "requires": ["node"],
  "run": "node run.js",
  "timeout": 30
}